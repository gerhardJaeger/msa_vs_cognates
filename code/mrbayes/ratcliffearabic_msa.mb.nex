#Nexus
Begin MrBayes;
    execute ratcliffearabic_msa.nex;
    prset brlenspr = clock:uniform;
    prset clockvarpr = igr;
    lset rates=gamma;
    lset covarion=yes;
    prset clockratepr=exp(1.0);
    lset coding=noabsencesites;
    mcmcp stoprule=no stopval=0.01 filename=output/ratcliffearabic_msa samplefreq=1000;
    mcmc ngen=10000000 nchains=2 nruns=2 append=no;
    sumt;
    sump;
    q;
end;
